<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/titulo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/inicio"
        android:layout_marginBottom="615dp"
        android:height="50dp"
        android:textColor="@color/black"
        android:text="Receitas para Engordar"
        android:textSize="25dp"
        android:gravity="center"
        />

    <ImageView
        android:id="@+id/receitasImagem"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/bc778d0c20c509a77c0c81c38c2abbe"
        android:translationY="110dp">
    </ImageView>

    <TextView
        android:id="@+id/receitasNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Frango Panado sem Farinha"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:translationX="90dp"
        android:onClick="frango"
        android:translationY="120dp">


    </TextView>

    <ImageView
        android:id="@+id/receitasImagem2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_weight="1"
        android:translationY="210dp"
        app:srcCompat="@drawable/bc778d0c20c509a77c0c81c38c2abbe"></ImageView>

    <TextView
        android:id="@+id/receitasNome2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peixe Panado"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:translationX="90dp"
        android:onClick="peixe"
        android:translationY="220dp">

    </TextView>

    <ImageView
        android:id="@+id/receitasImagem3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_weight="1"
        android:translationY="320dp"
        app:srcCompat="@drawable/bc778d0c20c509a77c0c81c38c2abbe"></ImageView>

    <TextView
        android:id="@+id/receitasNome3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carne Assada com Batata e Queijo"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:translationX="90dp"
        android:onClick="carne"
        android:translationY="330dp">

    </TextView>

    <ImageView
        android:id="@+id/receitasImagem4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_weight="1"
        android:translationY="430dp"
        android:onClick="mac1"
        app:srcCompat="@drawable/bc778d0c20c509a77c0c81c38c2abbe"></ImageView>

    <TextView
        android:id="@+id/receitasNome4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Macarrão Cremoso com Legumes"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:translationX="90dp"
        android:translationY="440dp">

    </TextView>

    <ImageView
        android:id="@+id/receitasImagem5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_weight="1"
        android:translationY="540dp"
        android:onClick="mac2"
        app:srcCompat="@drawable/bc778d0c20c509a77c0c81c38c2abbe"></ImageView>

    <TextView
        android:id="@+id/receitasNome5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Macarrão com brócolos e bacon"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:translationX="90dp"
        android:translationY="550dp">

    </TextView>

    <ImageView
        android:id="@+id/inicio"
        android:layout_width="52dp"
        android:layout_height="61dp"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginBottom="7dp"
        android:onClick="inicio"
        app:srcCompat="@drawable/imagem1" />

    <ImageView
        android:id="@+id/pdietas"
        android:layout_width="42dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="85dp"
        android:layout_marginBottom="10dp"
        android:onClick="pdietas"
        app:srcCompat="@drawable/bc778d0c20c509a77c0c81c38c2abbe" />

    <ImageView
        android:id="@+id/saude"
        android:layout_width="40dp"
        android:layout_height="43dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="10dp"
        android:onClick="saude"
        app:srcCompat="@drawable/planodesaude" />

    <ImageView
        android:id="@+id/plano"
        android:layout_width="51dp"
        android:layout_height="49dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="98dp"
        android:layout_marginRight="87dp"
        android:layout_marginBottom="9dp"
        android:onClick="plano"
        app:srcCompat="@drawable/plano" />

    <ImageView
        android:id="@+id/perfil2"
        android:layout_width="39dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="29dp"
        android:layout_marginBottom="9dp"
        android:onClick="perfil2"
        app:srcCompat="@drawable/_50689" />

    <Button
        android:id="@+id/buttonfrango"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:alpha="0."
        android:text="Button"
        android:onClick="frangopanado"
        android:translationY="100dp" />

    <Button
        android:id="@+id/buttonpeixe"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:alpha="0."
        android:text="Button"
        android:onClick="peixepanado"
        android:translationY="200dp" />
    <Button
        android:id="@+id/buttoncarne"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:alpha="0."
        android:text="Button"
        android:onClick="carneassada"
        android:translationY="310dp" />
    <Button
        android:id="@+id/buttonmac1"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:alpha="0."
        android:text="Button"
        android:onClick="mac1"
        android:translationY="420dp" />
    <Button
        android:id="@+id/buttonmac2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:alpha="0."
        android:text="Button"
        android:onClick="mac2"
        android:translationY="530dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="?attr/homeAsUpIndicator"
        android:onClick="volta" >

    </ImageView>


</RelativeLayout>
